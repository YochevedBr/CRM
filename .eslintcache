Route>\n                <Route path=\"/apt_details/:aptID\">\n                    <BootstrapNavbar/>\n                    <AptDetails/>\n                </Route>\n                <Route path=\"/update_apt/:aptID\">\n                    <BootstrapNavbar/>\n                    <UpdateOrAddApt/>\n                </Route>\n                <Route path=\"/add_apt\">\n                    <BootstrapNavbar/>\n                    <UpdateOrAddApt/>\n                </Route>\n                <Route path=\"/call_details/:callID\">\n                    <BootstrapNavbar/>\n                    <CallDetails/>\n                </Route>\n                \n                <Route path=\"/login\" component={Login} />\n                <Route path=\"/sign_up\" component={SignUp} />\n                \n                <Route component={Error} />\n            </Switch>\n        </Router>\n    );\n}\n// ReactDOM.render(routing, document.getElementsByClassName('App'));\n\nexport default routing;","/Users/pesiasimabrikman/Desktop/pessi/studies/software engineering/forth year/sem2/react/CRM/src/components/Purchases.js",[],"/Users/pesiasimabrikman/Desktop/pessi/studies/software engineering/forth year/sem2/react/CRM/src/components/Login.js",[],"/Users/pesiasimabrikman/Desktop/pessi/studies/software engineering/forth year/sem2/react/CRM/src/firebase.js",[],"/Users/pesiasimabrikman/Desktop/pessi/studies/software engineering/forth year/sem2/react/CRM/src/components/AddCustomer.js",[],"/Users/pesiasimabrikman/Desktop/pessi/studies/software engineering/forth year/sem2/react/CRM/src/components/HomePage.js",[],"/Users/pesiasimabrikman/Desktop/pessi/studies/software engineering/forth year/sem2/react/CRM/src/components/Products.js",["180","181","182","183","184"],"import React from \"react\"\nimport Product from \"./Product\"\nimport UpdateOrAddApt from './UpdateOrAddApt'\nimport Button from '@material-ui/core/Button';\n\nimport './Products.css'\n\nimport { useHistory } from \"react-router\";\n\nimport img 